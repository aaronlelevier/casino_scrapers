<article class="main">
  {{tickets/ticket-list
  count=count
  model=model
  page=page
  sort=sort
  search=search
  find=find
  id_in=id_in
  requested=requested
  page_size=page_size
  filterModel=filterModel
  filtersets=filtersets
  routeName=routeName
  noun="ticket"
  plural=(t "ticket.other")
  grid_title=(concat (t "ticket.status.in_progress") " " (t "ticket" count="2"))
  list_url="main.tickets.index"
  detail_url=(if FlexiLayout.isMobile "main.tickets.index.ticket" "main.tickets.index.ticket")
  add_model_url="main.tickets.index.new"
  search_placeholder=(t "ticket.search")
  add_model_text=(t "ticket.one")
  save_filterset=(action "save_filterset")
  hasActiveFilterSet=hasActiveFilterSet
  repository=repository
  }}
</article>
