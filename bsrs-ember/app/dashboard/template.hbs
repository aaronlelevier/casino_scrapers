<article class="main">
  <header>
  </header>
  <section class="docSection container-fluid">
    <div class="row">
      <div class="col-md-12">
        <section style="padding-bottom: 0">
          <div class="t-dashboard-text">{{dashboardData.settings.dashboard_text}}</div>
        </section>
        <section>
          {{#each model as |ticket|}}
            <div class="t-ticket-draft">{{ticket.number}}</div>
            {{#link-to "tickets.ticket" ticket.id tagName="button" class="btn btn-info t-ticket-status-draft-link"}}{{t "ticket.one"}}{{/link-to}}
          {{/each}}
        </section>
      </div>
    </div>
  </section>
</article>
<aside class="side-panel">
  <header>
    <h1></h1>
  </header>
</aside>
