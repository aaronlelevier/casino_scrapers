{{power-select-validation
  model=model
  valuePath="criteria"
  didValidate=didValidate
  index=index
}}
{{db-fetch-multi-select
  model=model
  multiAttr="criteria"
  multiAttrIds="criteria_ids"
  selectedAttr=model.criteria
  className="t-ticket-state-select"
  placeholder="admin.placeholder.available_filter.state"
  displayName="name"
  add_func="add_criteria"
  remove_func="remove_criteria"
  repository=repository
  searchMethod="findState"
  extra_params=extra_params
}}
