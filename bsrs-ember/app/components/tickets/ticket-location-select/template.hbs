{{power-select-validation
  model=model
  valuePath="criteria"
  didValidate=didValidate
  index=index
}}
{{db-fetch-multi-select
  model=model
  multiAttr="criteria"
  multiAttrIds="criteria_ids"
  selectedAttr=model.criteria
  className="t-ticket-location-select"
  placeholder=(t "admin.placeholder.location_filter_select")
  displayName="name"
  add_func="add_criteria"
  remove_func="remove_criteria"
  repository=repository
  searchMethod="findPersonsLocations"
  extra_params=extra_params
}}
