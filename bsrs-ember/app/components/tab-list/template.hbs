<nav class="tabs">
    <ul id="tabs" class="list-inline">
        {{#each tabs as |tab|}}
            {{#if tab.newModel}}
                {{#link-to tab.doc_route classNames="t-tab" tagName="li"}}
                <span class="t-tab-title">New {{tab.parent}}</span>
                <span class="close-wrap">
                    {{#if tab.model.isDirtyOrRelatedDirty}}
                    <span class="t-tab-close" {{action "closeTab" tab bubbles=false}}>{{fa-icon "circle" classNames="close-icon dirty"}}</span>
                    {{else}}
                    <span class="t-tab-close" {{action "closeTab" tab bubbles=false}}>{{fa-icon "times-circle" classNames="close-icon"}}</span>
                    {{/if}}
                </span>
                {{/link-to}}
            {{else}}
                {{#link-to tab.doc_route tab.id classNames="t-tab" tagName="li"}}
                <span class="t-tab-title">{{tab.parent}}</span>
                <span class="close-wrap">
                    {{#if tab.model.isDirtyOrRelatedDirty}}
                    <span class="t-tab-close" {{action "closeTab" tab bubbles=false}}>{{fa-icon "circle" classNames="close-icon dirty"}}</span>
                    {{else}}
                    <span class="t-tab-close" {{action "closeTab" tab bubbles=false}}>{{fa-icon "times-circle" classNames="close-icon"}}</span>
                    {{/if}}
                </span>
                {{/link-to}}
            {{/if}}
        {{/each}}
        <li class="new">
            <span>{{fa-icon "plus"}}</span>
        </li>
    </ul>
</nav>
