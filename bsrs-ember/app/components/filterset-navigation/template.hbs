{{#if showSaveFilterInput}}
  <div class="filtersets__item flex-item--none">
    {{mobile-save-filter filtersetName=filtersetName showSaveFilterInput=showSaveFilterInput}}
  </div>
{{/if}}
{{#each saved as |filterset|}}
  <div class="filtersets__item flex-item--none">{{#link-to filterset.endpoint_name (filterset-params filterset)}}{{filterset.name}}<i class="fa fa-minus-circle t-remove-filterset" aria-hidden="true" {{action "removeFilterSet" filterset.id}}></i>{{/link-to}}</div>
{{/each}}
