{{#unless readonly}}
  {{image-drop
    model=model
    droppedImage=droppedImage
    addHoverText=addHoverText
    changeHoverText=changeHoverText
    dropText=(t "general.drop.image")
    onError=onError
  }}
{{else}}
  {{photo-display
    image=droppedImage
    dataTestId=dataTestId
  }}
{{/unless}}

