{{#unless isExpanded}}
  {{work-orders/ticket-display-collapsed
    model=model
    indx=indx
    toggleIsExpanded=(action "toggleIsExpanded")
  }}
{{else}}
  {{work-orders/ticket-display-expanded
    model=model
    indx=indx
    permissions=permissions
    toggleIsExpanded=(action "toggleIsExpanded")
  }}
{{/unless}}
